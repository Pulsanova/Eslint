{
    "name": "@pulsanova/eslint-config-base",
    "version": "3.0.0",
    "license": "MIT",
    "type": "module",
    "main": "./index.js",
    "exports": "./index.js",
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:Pulsanova/Eslint.git",
        "directory": "packages/base"
    },
    "keywords": [
        "eslint",
        "eslintconfig"
    ],
    "scripts": {
        "find-new-rules-base": "eslint-find-rules --unused --no-error --deprecated --flatConfig ./index.js --ext .js .ts",
        "find-new-rules-json": "eslint-find-rules --unused --no-error --deprecated --no-core --flatConfig ./index.js --ext .json",
        "find-new-rules": "npm run --silent find-new-rules-base && npm run --silent find-new-rules-json",
        "inspect-config": "eslint --inspect-config --config ./index.js"
    },
    "dependencies": {
        "@eslint/json": "~0.13.1",
        "@stylistic/eslint-plugin": "~5.2.3",
        "@typescript-eslint/eslint-plugin": "~8.40.0",
        "@typescript-eslint/parser": "~8.40.0",
        "eslint-import-resolver-typescript": "~4.4.4",
        "eslint-plugin-import": "~2.32.0",
        "eslint-plugin-jsdoc": "~54.1.1",
        "eslint-plugin-unicorn": "~60.0.0"
    },
    "devDependencies": {
        "eslint": "^9.33.0",
        "eslint-find-rules": "^5.0.0"
    },
    "peerDependencies": {
        "eslint": "^9.33.0"
    }
}
