{
    "name": "@pulsanova/eslint-config-node",
    "version": "3.0.0",
    "license": "MIT",
    "type": "module",
    "main": "./index.js",
    "exports": {
        ".": "./index.js",
        "./legacy": "./legacy.js"
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:Pulsanova/Eslint.git",
        "directory": "packages/node"
    },
    "keywords": [
        "eslint",
        "eslintconfig"
    ],
    "scripts": {
        "find-new-rules": "eslint-find-rules --unused --no-error --deprecated --flatConfig ./index.js --ext .js .ts .cjs .mjs .cts .mts",
        "inspect-config": "eslint --inspect-config --config ./index.js"
    },
    "dependencies": {
        "@pulsanova/eslint-config-base": "3.0.0",
        "globals": "^16.3.0"
    },
    "devDependencies": {
        "eslint": "^9.33.0",
        "eslint-find-rules": "^5.0.0"
    },
    "peerDependencies": {
        "eslint": "^9.33.0"
    }
}
